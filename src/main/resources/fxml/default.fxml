<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.TilePane?>


<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.GridPane?>
<AnchorPane prefHeight="400.0" prefWidth="600.0"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.soft1841.sm.controller.DefaultController" >
    <HBox AnchorPane.topAnchor="0" AnchorPane.leftAnchor="0"
          AnchorPane.rightAnchor="0" AnchorPane.bottomAnchor="0">
        <StackPane styleClass="box">
        <ImageView fx:id="bookImg" fitWidth="650" fitHeight="350">
        </ImageView>
        </StackPane>
        <GridPane hgap="20" vgap="20" prefHeight="300" prefWidth="360" styleClass="box">
            <StackPane GridPane.columnIndex="0" GridPane.rowIndex="0"
                       styleClass="box,green-theme" prefWidth="150" prefHeight="130">
            </StackPane>

        </GridPane>
    </HBox>
</AnchorPane>